<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cricket">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".RunActivity"
            android:label="@string/title_activity_run"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".launching"
            android:theme="@style/AppTheme.NoToolBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".teamName"
            android:label="Team Details"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".players"
            android:label="Player Entry for Team 1"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".players2"
            android:label="Player Entry for Team 2"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".customDialog"
            android:label="Toss"
            android:theme="@style/AppTheme" />
        <activity android:name=".playerlist1" />
        <activity android:name=".playerlist2"></activity>
    </application>

</manifest>